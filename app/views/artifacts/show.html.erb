<div align="center">
  <%- model_class = Artifact -%>
  <div class="page-header">
    <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
  </div>
</div>

<dl class="dl-horizontal col-md-offset-4">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @artifact.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:key) %>:</strong></dt>
  <dd><%= @artifact.key %></dd>
  <dt><strong><%= model_class.human_attribute_name(:project_id) %>:</strong></dt>
  <dd><%= @artifact.project_id %></dd>
</dl>

<div align="center">
  <div>
    <iframe></iframe>
  </div>
  <%= link_to "Download", @artifact.doc.url, :class => 'btn btn-default' %>
</div>
