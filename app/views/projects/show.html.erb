<% model_class = Project %>
<div class="page-header">
  <h3><%= @project.title %></h3>
  <h3><small>Organisation - <%= tenant_name @project.tenant_id %></small></h3>
</div

<h3>Artifacts</h3>
<%= render 'artifacts/list', artifacts: @project.artifacts %>
<br />

<%= link_to '< Back to Organization Home', root_path, :class => 'btn btn-default' %>

<%= link_to t('.edit', :default => t("helpers.links.edit")),
    edit_tenant_project_path(project: @project, tenant: @project.tenant_id), :class => 'btn btn-default' %>

<%= link_to t('.destroy', :default => t("helpers.links.destroy")), tenant_project_path(@project, @project.tenant_id),
    :method => 'delete', :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm",
    :default => 'Are you sure?')) }, :class => 'btn btn-danger' %>


